{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Robots-Tag",
                    "value": "noai, noimageai"
                }
            ]
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "has": [
                {
                    "type": "header",
                    "key": "user-agent",
                    "value": ".*DotBot.*"
                }
            ],
            "status": 403
        }
    ]
}
